<a href="/teacher/viewall" class="btn btn-primary" style="margin-top:5px;">Back</a>
<h4>Editing Form for Student :<%= user.name %> </h4>
<form method="POST" class="addstudent" autocomplete="off" id="editstudent">
    <div class="mb-3">
      <label  class="form-label">Name : </label>
      <input type="text" name="name" required class="form-control" value="<%= user.name  %> " >
    </div>
    <div class="mb-3">
        <label class="form-label">Roll No. : </label>
        <input type="text" name="roll" value="<%= user.roll  %> " required class="form-control" disabled>
      </div>     
     
      <div class="mb-3">
        <label class="form-label">Date of Birth : </label>
        <input type="text" name="dob" value="<%= user.dob  %> " required class="form-control">
      </div>
      <div class="mb-3">
        <label class="form-label">Score : </label>
        <input type="text" name="score" value="<%= user.score  %> " required class="form-control">
      </div>
    <button type="submit" class="btn btn-primary">Update</button>
</form>


<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.0/jquery.min.js" integrity="sha512-3gJwYpMe3QewGELv8k/BX9vcqhryRdzRMxVfq6ngyWXwo03GFEzjsUm8Q7RZcHPHksttq7/GFoxjCVUjkjvPdw==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
